{app, [{{appid}}}]}.
{erl_opts, [debug_info]}.

{xref_checks, [undefined_function_calls, undefined_functions,
               locals_not_used, deprecated_function_calls, deprecated_functions]}.

{cover_enabled, true}.
{clean_files, ["./deps/*/ebin/*.beam", "./apps/*/ebin/*.beam", "ebin/*.beam", "priv/log/*"]}.

{deps_dir, ["./deps"]}.
{sub_dirs, ["./deps", "apps/{{appid}}", "rel"]}.

{deps, []}.
